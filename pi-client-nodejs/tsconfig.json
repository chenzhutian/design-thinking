{
	// See http://go.microsoft.com/fwlink/?LinkId=759670
	// for the documentation about the jsconfig.json format
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"sourceMap": true,
		"noImplicitAny": false,
		"removeComments": true,
		"preserveConstEnums": true,
		"outDir": "./dist/",
		"allowJs": true
	},
	"exclude": [
		"dist",
		"build.js",
		"node_modules",
		"bower_components",
		"jspm_packages",
		"tmp",
		"temp"
	]
}