{
    "extends": "airbnb/base",
    "rules": {
        "semi": 2,
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "no-param-reassign": [
            "error",
            {
                "props": false
            }
        ],
        "no-unused-vars": [
            "error",
            {
                "args": "none"
            }
        ]
    }
}